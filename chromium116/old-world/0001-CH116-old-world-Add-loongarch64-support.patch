+        dst_scale = __lasx_xvadd_w(dst_scale, __lasx_xvsrli_w(dst_scale, 8));
+        dst_scale = __lasx_xvsrli_w(dst_scale, 8);
+        // Duplicate scales into 2x16-bit pattern per pixel.
